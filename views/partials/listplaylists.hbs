{{#each playlists}}
  <section class="ui segment">
    <h2 class="ui header">
      {{title}}
    </h2>
    <a href="/playlist/{{id}}" class="ui icon button"><i class = "folder open icon"></i></a>
    <a href="/dashboard/deleteplaylist/{{id}}" class="ui red icon button"><i class = "trash outline icon"></i></a>
  </section>
{{/each}}